
DROP TABLE  IF EXISTS `d60701781`.`calendardetails`;
CREATE TABLE  `d60701781`.`calendardetails` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `title` text NOT NULL,
   start datetime NOT NULL,
   end datetime NOT NULL,
   `createdby` bigint(20) NOT NULL,
   isdeleted int(11) NOT NULL,
   `createddate` datetime NOT NULL,
  PRIMARY KEY (`calendarid`)
) ENGINE=MyISAM AUTO_INCREMENT=102722 DEFAULT CHARSET=utf8


CREATE TABLE  `d60701781`.`stickycomments` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `stickyid` bigint(20) NOT NULL,
  `stickycomment` text NOT NULL,
  `commentby` varchar(500) NOT NULL,
  `createdby` bigint(20) NOT NULL,
  `isdeleted` int(11) NOT NULL,
  `createddate` datetime NOT NULL,
  PRIMARY KEY (`id`),
  KEY `FK_sticky` (`stickyid`),
  CONSTRAINT `FK_sticky` FOREIGN KEY (`stickyid`) REFERENCES `stickynotes` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=102740 DEFAULT CHARSET=utf8

CREATE TABLE  `d60701781`.`stickynotes` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `stickytext` text NOT NULL,
  `name` varchar(60) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `color` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT 'yellow',
  `xyz` varchar(20) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `createdby` bigint(20) NOT NULL,
  `isdeleted` int(11) NOT NULL,
  `createddate` datetime NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=102746 DEFAULT CHARSET=utf8